Child) {\n        let formattedGifts = {\n          uid: uuidv4(),\n          gifts: [],\n        };\n\n        v.list.map((v, i) => {\n          return formattedGifts.gifts.push({ gift: v });\n        });\n\n        addChildGiftList(letterToSanta.currentChild, formattedGifts)\n          .then()\n          .catch(console.log);\n      }\n    });\n\n    setShowLogin(true);\n  };\n\n  const loginSubmit = (event) => {\n    event.preventDefault();\n\n    let userDetails = {\n      username: event.target.username.value,\n      password: event.target.password.value,\n    };\n\n    loginUser(userDetails)\n      .then(() => {\n        letterToSanta.parentMode = true;\n        dispatch({\n          type: \"setLetterToSanta\",\n          data: letterToSanta,\n        });\n      })\n      .catch(() => {\n        setErrors([1]);\n      });\n  };\n\n  return (\n    <div>\n      <div className=\"row row-cols-2\">\n        <div className=\"col\">\n          <img\n            className=\"santa\"\n            src={window.location.origin + \"/images/dabbingSanta.png\"}\n            alt=\"santa\"\n          />\n        </div>\n        <div className=\"col\">\n          <h3>My List</h3>\n          <div className=\"childList\">\n            {letterToSanta.children.map((children, i) => {\n              if (children.uid === letterToSanta.currentChild) {\n                return children.list.map((present, i) => {\n                  return <li key={i}>{present}</li>;\n                });\n              }\n            })}\n          </div>\n        </div>\n      </div>\n      <p className=\"my-3\">\n        Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sed tempus urna\n        magna, a luctus arcu dignissim at. Sed et lacus in urna auctor egestas.\n        Mauris mollis mauris ut augue tempor iaculis. Curabitur vitae erat urna.\n        Vivamus tristique cursus lacinia.\n      </p>\n\n      <form\n        onSubmit={addPresent}\n        className=\"d-flex justify-content-center align-items-center flex-column\"\n      >\n        <h3>What can santa get for you?</h3>\n        <input\n          required\n          type=\"text\"\n          name=\"present\"\n          placeholder=\"what would you like?\"\n        ></input>\n\n        <input\n          className=\"christmasInputButton buttonFont m-2\"\n          type=\"submit\"\n          value=\"Add Present\"\n        ></input>\n      </form>\n\n      {/* <button onClick={finalizeList}>All Done</button>  */}\n      <div className=\"d-flex justify-content-center align-items-center my-3\">\n        <ChristmasButton text=\"All Done\" onClick={finalizeList} />\n      </div>\n      <p>before you press this button go get your parent</p>\n      {/* implement password and username check */}\n      {showLogin ? (\n        <div className=\"secondaryLogin\">\n          {errors.length > 0 ? <h5 className=\"errors\">Thats not Right</h5> : \"\"}\n\n          <form\n            className=\"login d-flex flex-column align-items-center\"\n            onSubmit={loginSubmit}\n          >\n            <p>\n              Go get your parent and get them to login to confirm your letter to\n              santa!\n            </p>\n            <label>Username</label>\n            <input\n              required\n              className=\"username mb-2\"\n              type=\"text\"\n              name=\"username\"\n              placeholder=\"Enter a username\"\n            ></input>\n\n            <label>Password</label>\n            <input\n              required\n              className=\"password mb-2\"\n              type=\"password\"\n              name=\"password\"\n              placeholder=\"Enter a password\"\n            ></input>\n\n            <input\n              className=\"christmasInputButton m-2\"\n              type=\"submit\"\n              value=\"Login\"\n            ></input>\n          </form>\n        </div>\n      ) : (\n        \"\"\n      )}\n    </div>\n  );\n};\n\nexport default LetterToSantaChild;\n","/mnt/c/Users/Jordon/projects/javascript/santaMERN/Client/santa-client/src/components/letterToSanta/LetterToSantaParent.js",["109"],"import { useGlobalState } from \"../../config/store\";\nimport AddChildForm from \"./AddChildForm\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport Popover from \"react-bootstrap/Popover\";\nimport OverlayTrigger from \"react-bootstrap/OverlayTrigger\";\nimport React, { useState, useEffect } from \"react\";\nimport {\n  getChildren,\n  getChildGiftList,\n} from \"../../services/childGiftListServices\";\nimport { Redirect } from \"react-router-dom\";\nimport \"../../styles/pages/letterToSanta.scss\";\n\nconst LetterToSantaParent = () => {\n  const { store, dispatch } = useGlobalState();\n  const { letterToSanta } = store;\n\n  let [viewed, setViewed] = useState([]);\n\n  // use effect extracts data from data base and puts it into\n  // rteadable gloabl state format\n  useEffect(() => {\n    // extracts names and uid\n    getChildren(localStorage.loggedInUser)\n      .then((data) => {\n        data.children.map(async (child, i) => {\n          let formattedObj = {\n            list: [],\n            name: child.name,\n            uid: child.childUid,\n          };\n\n          // extacts list\n          await getChildGiftList(child.childUid).then((listData) => {\n            if (listData[0]) {\n              listData[0].gifts.map((gift, i) => {\n                 if (gift.gift) {\n                  return formattedObj.list.push(gift.gift);\n                }\n              });\n            }\n          });\n\n\n          viewed.push(formattedObj);\n          setViewed(viewed);\n          letterToSanta.children = viewed;\n\n          dispatch({\n            type: \"setLetterToSanta\",\n            data: letterToSanta,\n          });\n        });\n      })\n      .catch(console.log);\n  }, [dispatch,letterToSanta,viewed]);\n\n  // makes a form to be filled out with child details\n  // cannot make another form if there already is another\n  // prompts user to fill out existing form\n  const addChildForm = (event) => {\n    event.preventDefault();\n\n    letterToSanta.addForm = true;\n\n    dispatch({\n      type: \"setLetterToSanta\",\n      data: letterToSanta,\n    });\n  };\n\n  // changes the page into child mode and enables child list creation\n  // for specified child\n  const createList = (event) => {\n    event.preventDefault();\n\n    letterToSanta.parentMode = false;\n\n    letterToSanta.currentChild = event.target.getAttribute(\"uid\");\n\n    dispatch({\n      type: \"setLetterToSanta\",\n      data: letterToSanta,\n    });\n  };\n\n  return (\n    <div>\n      {/* checks if user is logged in */}\n      {!localStorage.loggedInUser ? <Redirect to=\"/login\" /> : \"\"}\n      <h1 className=\"my-5\">\n        <u>Parent Mode</u>\n      </h1>\n\n      <div className=\"row\">\n        <div className=\"col d-flex flex-column aling-items-center justify-content-center\">\n          <h3>How it works?</h3>\n          <p>\n            Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sed tempus\n            urna magna, a luctus arcu dignissim at. Sed et lacus in urna auctor\n            egestas. Mauris mollis mauris ut augue tempor iaculis. Curabitur\n            vitae erat urna. Vivamus tristique cursus lacinia.\n          </p>\n        </div>\n\n        <div className=\"col\">\n          <h3>Manage Children</h3>\n          <div className=\"childrenDiv\">\n            {/* creates popout elements containing relevent childs list  */}\n            <div className=\"row row-cols-2\">\n              {letterToSanta.children.length > 0 ? (\n                letterToSanta.children.map((v, i) => {\n                  // put get child gift list here\n                  // child name is also needed so getChildren too\n                  const popover = (\n                    <Popover id={i}>\n                      <Popover.Title as=\"h3\">{v.name}'s List</Popover.Title>\n                      <Popover.Content>\n                        {v.list.map((gift, i) => {\n                          return <li>{gift}</li>;\n                        })}\n                      </Popover.Content>\n                    </Popover>\n                  );\n\n                  return (\n                    <div\n                      key={i}\n                      className=\"d-flex justify-content-center childAndButton\"\n                    >\n                      <div className=\"col m-2\">\n                        <p>{v.name}</p>\n                        {v.list.length > 0 ? (\n                          <div className=\"d-flex flex-column justify-content-center align-items-center\">\n                            <OverlayTrigger\n                              trigger=\"click\"\n                              placement=\"right\"\n                              overlay={popover}\n                            >\n                              <button\n                                uid={v.uid}\n                                className=\"christmasButtonLetter\"\n                              >\n                                View List\n                              </button>\n                            </OverlayTrigger>\n                          </div>\n                        ) : (\n                          <div className=\"d-flex flex-column justify-content-center align-items-center\">\n                            <button\n                              uid={v.uid}\n                              onClick={createList}\n                              className=\"christmasButtonLetter\"\n                            >\n                              Create List\n                            </button>\n                          </div>\n                        )}\n                      </div>\n                    </div>\n                  );\n                })\n              ) : (\n                <div> None Here Yet </div>\n              )}\n            </div>\n            <div className=\"d-flex flex-column justify-content-center align-items-center my-2\">\n              <button className=\"christmasButtonLetter\" onClick={addChildForm}>\n                Add Child\n              </button>\n            </div>\n\n            {letterToSanta.addForm ? <AddChildForm /> : \"\"}\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default LetterToSantaParent;\n","/mnt/c/Users/Jordon/projects/javascript/santaMERN/Client/santa-client/src/components/letterToSanta/AddChildForm.js",[],"/mnt/c/Users/Jordon/projects/javascript/santaMERN/Client/santa-client/src/services/childGiftListServices.js",[],"/mnt/c/Users/Jordon/projects/javascript/santaMERN/Client/santa-client/src/services/authServices.js",[],"/mnt/c/Users/Jordon/projects/javascript/santaMERN/Client/santa-client/src/index.js",[],{"ruleId":"110","severity":1,"message":"111","line":1,"column":1,"nodeType":"112","endLine":26,"endColumn":2},{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","severity":1,"message":"118","line":21,"column":9,"nodeType":"119","endLine":52,"endColumn":4},{"ruleId":"120","severity":1,"message":"121","line":25,"column":25,"nodeType":"122","messageId":"123","endLine":25,"endColumn":27},{"ruleId":"120","severity":1,"message":"121","line":27,"column":30,"nodeType":"122","messageId":"123","endLine":27,"endColumn":32},{"ruleId":"113","replacedBy":"124"},{"ruleId":"115","replacedBy":"125"},{"ruleId":"113","replacedBy":"126"},{"ruleId":"115","replacedBy":"127"},{"ruleId":"120","severity":1,"message":"128","line":19,"column":39,"nodeType":"122","messageId":"129","endLine":19,"endColumn":41},{"ruleId":"120","severity":1,"message":"121","line":36,"column":39,"nodeType":"122","messageId":"123","endLine":36,"endColumn":41},{"ruleId":"120","severity":1,"message":"128","line":90,"column":55,"nodeType":"122","messageId":"129","endLine":90,"endColumn":57},{"ruleId":"120","severity":1,"message":"128","line":36,"column":47,"nodeTy